@prefix NCIT: <http://purl.obolibrary.org/obo/NCIT_> .
@prefix pfhub: <https://w3id.org/usnistgov/pfhub-schema/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

pfhub:BenchmarkResult a sh:NodeShape ;
    sh:closed true ;
    sh:description "Root and context for this Benchmark Problem solution." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Informal or human-memorable identifier that may collide with others." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path schema:name ],
        [ sh:class schema:SoftwareApplication ;
            sh:description "A computer program or library" ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path schema:SoftwareApplication ],
        [ sh:class schema:Person ;
            sh:description "Collection of people who worked on this solution." ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path schema:contributor ],
        [ sh:description "Text description, abstract, or narrative." ;
            sh:maxCount 1 ;
            sh:order 7 ;
            sh:path schema:abstract ],
        [ sh:class schema:Dataset ;
            sh:description "Collection of outputs and summary data from a simulation." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 6 ;
            sh:path pfhub:results ],
        [ sh:class pfhub:Benchmark ;
            sh:description "Details of the Benchmark Problem." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path pfhub:benchmark ],
        [ sh:description "Unique identifier that must not collide with any other." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path schema:identifier ],
        [ sh:class schema:SoftwareSourceCode ;
            sh:description "Source code implementing the solution." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 5 ;
            sh:path pfhub:implementation ] ;
    sh:targetClass pfhub:BenchmarkResult .

schema:Dataset a sh:NodeShape ;
    sh:closed true ;
    sh:description "Runtime information and output from a simulation." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class schema:softwareRequirements ;
            sh:description "Computer hardware used to run the simulation." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 3 ;
            sh:path pfhub:hardware ],
        [ sh:description "Memory capacity required for this simulation." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path pfhub:memory_in_KB ],
        [ sh:class schema:DigitalDocument ;
            sh:description "Tabular output from a simulation." ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path pfhub:data_table ],
        [ sh:description "Amount of time required to execute the simulation." ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path schema:Number ],
        [ sh:class schema:DigitalDocument ;
            sh:description "Visualization output from a simulation." ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path pfhub:data_visualization ],
        [ sh:class schema:DigitalDocument ;
            sh:description "Raw data output from a simulation." ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path pfhub:data_raw ] ;
    sh:targetClass schema:Dataset .

schema:Person a sh:NodeShape ;
    sh:closed true ;
    sh:description "Someone who contributed to this solution." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Unique identifier that must not collide with any other." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path schema:identifier ],
        [ sh:description "A person's identifier on a website, without the \"at\" symbol." ;
            sh:order 2 ;
            sh:path schema:identifier ],
        [ sh:description "Institution, company, or agency a person works with." ;
            sh:order 3 ;
            sh:path pfhub:affiliation ],
        [ sh:description "A person's electronic mailing address." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path schema:email ;
            sh:pattern "^\\S+@[\\S+\\.]+\\S+" ],
        [ sh:description "Informal or human-memorable identifier that may collide with others." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path schema:name ] ;
    sh:targetClass schema:Person .

schema:SoftwareSourceCode a sh:NodeShape ;
    sh:closed true ;
    sh:description "Link to the authors' implementation of the benchmark problem." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Informal or human-memorable identifier that may collide with others." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path schema:name ],
        [ sh:description "A repository of source code." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path schema:codeRepository ],
        [ sh:description "Unique identifier that must not collide with any other." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path schema:identifier ] ;
    sh:targetClass schema:SoftwareSourceCode .

pfhub:Benchmark a sh:NodeShape ;
    sh:closed true ;
    sh:description "Information about the specific Benchmark solved." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Alphanumerical identifier for part of a Benchmark Problem." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path schema:identifier ;
            sh:pattern "^\\a\\d" ],
        [ sh:description "Unique identifier that must not collide with any other." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path schema:identifier ],
        [ sh:description "Informal or human-memorable identifier that may collide with others." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path schema:name ],
        [ sh:description "Semantic version of a software project." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path schema:softwareVersion ] ;
    sh:targetClass pfhub:Benchmark .

schema:SoftwareApplication a sh:NodeShape ;
    sh:closed true ;
    sh:description "A software product, download, or repository." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Unique identifier that must not collide with any other." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path schema:identifier ],
        [ sh:description "A repository of source code." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path schema:codeRepository ],
        [ sh:description "Informal or human-memorable identifier that may collide with others." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path schema:name ],
        [ sh:description "Semantic version of a software project." ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path schema:softwareVersion ],
        [ sh:description "Link to a download URL." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path schema:downloadUrl ] ;
    sh:targetClass schema:SoftwareApplication .

schema:softwareRequirements a sh:NodeShape ;
    sh:closed true ;
    sh:description "Summary of the hardware used to execute the simulation." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Number of parallel cores or threads of execution used by the simulation on a CPU, GPU, or accelerator device." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path NCIT:C64194 ],
        [ sh:description "Number of machines allocated for the simulation in a computer cluster environment." ;
            sh:maxCount 1 ;
            sh:order 2 ;
            sh:path NCIT:C18132 ],
        [ sh:class schema:softwareRequirements ;
            sh:description "Computer processor architecture required for this simulation." ;
            sh:nodeKind sh:BlankNode ;
            sh:order 0 ;
            sh:path pfhub:architecture ] ;
    sh:targetClass schema:softwareRequirements .

schema:DigitalDocument a sh:NodeShape ;
    sh:closed false ;
    sh:description "A generic electronic information container." ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Unique identifier that must not collide with any other." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path schema:identifier ],
        [ sh:description "Format of data contained in a file." ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path schema:encodingFormat ],
        [ sh:description "Informal or human-memorable identifier that may collide with others." ;
            sh:maxCount 1 ;
            sh:order 1 ;
            sh:path schema:name ],
        [ sh:description "Column names applied to tabular data." ;
            sh:order 2 ;
            sh:path pfhub:columns ;
            sh:pattern "^\\S+" ] ;
    sh:targetClass schema:DigitalDocument .

