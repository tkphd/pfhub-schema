@prefix IAO: <http://purl.obolibrary.org/obo/IAO_> .
@prefix NCIT: <http://purl.obolibrary.org/obo/NCIT_> .
@prefix SRAO: <http://www.fairsharing.org/ontology/subject/SRAO_> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pav: <http://purl.org/pav/> .
@prefix pfhub: <https://w3id.org/usnistgov/pfhub-schema/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

linkml:SubsetDefinition a owl:Class ;
    rdfs:label "subset_definition" .

linkml:TypeDefinition a owl:Class ;
    rdfs:label "type_definition" .

linkml:topValue a owl:DatatypeProperty ;
    rdfs:label "value" .

<https://w3id.org/usnistgov/pfhub-schema> a owl:Ontology ;
    rdfs:label "pfhub_schema" ;
    IAO:0000700 pfhub:Benchmark,
        pfhub:BenchmarkResult,
        pfhub:ComputeResource,
        pfhub:Contributor,
        pfhub:File,
        pfhub:Results,
        pfhub:Software,
        pfhub:SourceCode ;
    dcterms:license "https://www.nist.gov/open/license" ;
    dcterms:title "PFHub Benchmark Results" ;
    pav:version "0.1.0" ;
    rdfs:seeAlso "https://pages.nist.gov/pfhub",
        "https://pages.nist.gov/pfhub-schema" ;
    linkml:generation_date "2023-03-16T16:18:41" ;
    linkml:metamodel_version "1.7.0" ;
    linkml:source_file "pfhub_schema.yaml" ;
    linkml:source_file_date "2023-03-16T16:18:16" ;
    linkml:source_file_size 8000 .

pfhub:BenchmarkResult a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "BenchmarkResult" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty schema:identifier ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:abstract ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass pfhub:Results ;
            owl:onProperty pfhub:results ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom pfhub:Contributor ;
            owl:onProperty schema:contributor ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass pfhub:SourceCode ;
            owl:onProperty pfhub:implementation ],
        [ a owl:Restriction ;
            owl:allValuesFrom pfhub:Software ;
            owl:onProperty schema:SoftwareApplication ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Date ;
            owl:onProperty schema:dateCreated ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass pfhub:Benchmark ;
            owl:onProperty pfhub:benchmark ] ;
    skos:definition "Root and context for this Benchmark Problem solution." .

pfhub:affiliation a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "affiliation" ;
    rdfs:range linkml:String ;
    skos:definition "Institution, company, or agency a person works with." .

pfhub:benchmark a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "benchmark" ;
    rdfs:range pfhub:Benchmark ;
    skos:definition "Details of the Benchmark Problem." .

pfhub:columns a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "columns" ;
    rdfs:range linkml:String ;
    skos:definition "Column names applied to tabular data." .

pfhub:hardware a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "hardware" ;
    rdfs:range pfhub:ComputeResource ;
    skos:definition "Computer hardware used to run the simulation." .

pfhub:implementation a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "implementation" ;
    rdfs:range pfhub:SourceCode ;
    skos:definition "Source code implementing the solution." .

pfhub:memory_in_kb a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "memory_in_kb" ;
    rdfs:range linkml:Integer ;
    skos:definition "Memory capacity required for this simulation." .

pfhub:raw_data a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "raw_data" ;
    rdfs:range pfhub:File ;
    skos:definition "Raw data output from a simulation." .

pfhub:results a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "results" ;
    rdfs:range pfhub:Results ;
    skos:definition "Collection of outputs and summary data from a simulation." .

pfhub:viz_data a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "viz_data" ;
    rdfs:range pfhub:File ;
    skos:definition "Visualization output from a simulation." .

NCIT:C18132 a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "nodes" ;
    rdfs:range linkml:Integer ;
    skos:definition "Number of machines allocated for the simulation in a computer cluster environment." ;
    skos:exactMatch NCIT:C18132 .

NCIT:C64194 a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "cores" ;
    rdfs:range linkml:Integer ;
    skos:definition "Number of parallel cores or threads of execution used by the simulation on a CPU, GPU, or accelerator device." ;
    skos:exactMatch NCIT:C64194 .

schema:Number a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "time_in_s" ;
    rdfs:range linkml:Integer ;
    skos:definition "Amount of time required to execute the simulation." ;
    skos:exactMatch schema:Number .

schema:abstract a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "summary" ;
    rdfs:range linkml:String ;
    skos:definition "Text description, abstract, or narrative." ;
    skos:exactMatch schema:abstract .

schema:contributor a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "contributors" ;
    rdfs:range pfhub:Contributor ;
    skos:definition "Collection of people who worked on this solution." ;
    skos:exactMatch schema:contributor .

schema:dateCreated a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "date_created" ;
    rdfs:range linkml:Date ;
    skos:definition "Day when an item was uploaded, released, or published." ;
    skos:exactMatch schema:dateCreated .

schema:downloadUrl a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "download" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "Link to a download URL." ;
    skos:exactMatch schema:downloadUrl .

schema:email a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "email" ;
    rdfs:range linkml:String ;
    skos:definition "A person's electronic mailing address." ;
    skos:exactMatch schema:email .

schema:encodingFormat a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "format" ;
    rdfs:range linkml:String ;
    skos:definition "Format of data contained in a file." ;
    skos:exactMatch schema:encodingFormat .

schema:member a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "handle" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "A person's identifier on a website, without the \"at\" symbol." ;
    skos:exactMatch schema:member .

SRAO:0000258 a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "architecture" ;
    rdfs:range linkml:String ;
    skos:definition "Computer processor architecture required for this simulation." ;
    skos:exactMatch SRAO:0000258 .

pfhub:csv_data a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "csv_data" ;
    rdfs:range pfhub:File ;
    skos:definition "Tabular output from a simulation." .

schema:SoftwareApplication a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "framework" ;
    rdfs:range pfhub:Software ;
    skos:definition "A computer program or library" ;
    skos:exactMatch schema:SoftwareApplication .

schema:codeRepository a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "repository" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "A repository of source code." ;
    skos:exactMatch schema:codeRepository .

schema:softwareVersion a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "version" ;
    rdfs:range linkml:String ;
    skos:definition "Semantic version of a software project." ;
    skos:exactMatch schema:softwareVersion .

pfhub:Benchmark a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Benchmark" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:softwareVersion ],
        [ a owl:Restriction ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty schema:identifier ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:onClass linkml:String ;
            owl:onProperty schema:version ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "Information about the specific Benchmark solved." .

pfhub:ComputeResource a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "ComputeResource" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty NCIT:C18132 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty SRAO:0000258 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty NCIT:C64194 ] ;
    skos:definition "Summary of the hardware used to execute the simulation." ;
    skos:exactMatch schema:softwareRequirements .

pfhub:Contributor a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Contributor" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty schema:identifier ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty pfhub:affiliation ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Uriorcurie ;
            owl:onProperty schema:member ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:name ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:email ] ;
    skos:definition "Someone who contributed to this solution." ;
    skos:exactMatch schema:Person .

pfhub:Results a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Results" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom pfhub:ComputeResource ;
            owl:onProperty pfhub:hardware ],
        [ a owl:Restriction ;
            owl:allValuesFrom pfhub:File ;
            owl:onProperty pfhub:raw_data ],
        [ a owl:Restriction ;
            owl:allValuesFrom pfhub:File ;
            owl:onProperty pfhub:viz_data ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty pfhub:memory_in_kb ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Integer ;
            owl:onProperty schema:Number ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom pfhub:File ;
                        owl:onProperty pfhub:csv_data ] [ a owl:Restriction ;
                        owl:onProperty pfhub:csv_data ;
                        owl:someValuesFrom pfhub:File ] ) ] ;
    skos:definition "Runtime information and output from a simulation." ;
    skos:exactMatch schema:Dataset .

pfhub:Software a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Software" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:name ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:softwareVersion ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty schema:codeRepository ],
        [ a owl:Restriction ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty schema:identifier ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty schema:downloadUrl ] ;
    skos:definition "A software product, download, or repository." ;
    skos:exactMatch schema:SoftwareApplication .

pfhub:SourceCode a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "SourceCode" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty schema:identifier ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty schema:codeRepository ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:name ] ;
    skos:definition "Link to the authors' implementation of the benchmark problem." ;
    skos:exactMatch schema:SoftwareSourceCode .

schema:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    rdfs:range linkml:String ;
    skos:definition "Informal or human-memorable identifier that may collide with others." ;
    skos:exactMatch schema:name .

schema:identifier a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "Unique identifier that must not collide with any other." ;
    skos:exactMatch schema:identifier .

linkml:ClassDefinition a owl:Class ;
    rdfs:label "class_definition" .

pfhub:File a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "File" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass linkml:Uriorcurie ;
            owl:onProperty schema:identifier ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:encodingFormat ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty pfhub:columns ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty schema:name ] ;
    skos:definition "A generic electronic information container." ;
    skos:exactMatch schema:DigitalDocument .

linkml:SlotDefinition a owl:Class ;
    rdfs:label "slot_definition" .


