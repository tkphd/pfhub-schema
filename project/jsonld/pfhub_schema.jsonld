{
  "name": "pfhub_schema",
  "description": "Schema for Phase-field Simulation and Benchmark Results schema in LinkML.",
  "title": "PFHub Benchmark Results",
  "see_also": [
    "https://pages.nist.gov/pfhub",
    "https://pages.nist.gov/pfhub-schema"
  ],
  "id": "https://w3id.org/usnistgov/pfhub-schema",
  "version": "0.1.0",
  "imports": [
    "linkml:types"
  ],
  "license": "https://www.nist.gov/open/license",
  "prefixes": [
    {
      "prefix_prefix": "pfhub",
      "prefix_reference": "https://w3id.org/usnistgov/pfhub-schema/"
    },
    {
      "prefix_prefix": "linkml",
      "prefix_reference": "https://w3id.org/linkml/"
    }
  ],
  "default_curi_maps": [
    "semweb_context"
  ],
  "default_prefix": "pfhub",
  "default_range": "string",
  "types": [
    {
      "name": "string",
      "definition_uri": "https://w3id.org/linkml/String",
      "description": "A character string",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "exact_mappings": [
        "schema:Text"
      ],
      "base": "str",
      "uri": "http://www.w3.org/2001/XMLSchema#string",
      "@type": "TypeDefinition"
    },
    {
      "name": "integer",
      "definition_uri": "https://w3id.org/linkml/Integer",
      "description": "An integer",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "exact_mappings": [
        "schema:Integer"
      ],
      "base": "int",
      "uri": "http://www.w3.org/2001/XMLSchema#integer",
      "@type": "TypeDefinition"
    },
    {
      "name": "boolean",
      "definition_uri": "https://w3id.org/linkml/Boolean",
      "description": "A binary (true or false) value",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "exact_mappings": [
        "schema:Boolean"
      ],
      "base": "Bool",
      "uri": "http://www.w3.org/2001/XMLSchema#boolean",
      "repr": "bool",
      "@type": "TypeDefinition"
    },
    {
      "name": "float",
      "definition_uri": "https://w3id.org/linkml/Float",
      "description": "A real number that conforms to the xsd:float specification",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "exact_mappings": [
        "schema:Float"
      ],
      "base": "float",
      "uri": "http://www.w3.org/2001/XMLSchema#float",
      "@type": "TypeDefinition"
    },
    {
      "name": "double",
      "definition_uri": "https://w3id.org/linkml/Double",
      "description": "A real number that conforms to the xsd:double specification",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "close_mappings": [
        "schema:Float"
      ],
      "base": "float",
      "uri": "http://www.w3.org/2001/XMLSchema#double",
      "@type": "TypeDefinition"
    },
    {
      "name": "decimal",
      "definition_uri": "https://w3id.org/linkml/Decimal",
      "description": "A real number with arbitrary precision that conforms to the xsd:decimal specification",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "broad_mappings": [
        "schema:Number"
      ],
      "base": "Decimal",
      "uri": "http://www.w3.org/2001/XMLSchema#decimal",
      "@type": "TypeDefinition"
    },
    {
      "name": "time",
      "definition_uri": "https://w3id.org/linkml/Time",
      "description": "A time object represents a (local) time of day, independent of any particular day",
      "notes": [
        "URI is dateTime because OWL reasoners do not work with straight date or time"
      ],
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "exact_mappings": [
        "schema:Time"
      ],
      "base": "XSDTime",
      "uri": "http://www.w3.org/2001/XMLSchema#dateTime",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "date",
      "definition_uri": "https://w3id.org/linkml/Date",
      "description": "a date (year, month and day) in an idealized calendar",
      "notes": [
        "URI is dateTime because OWL reasoners don't work with straight date or time"
      ],
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "exact_mappings": [
        "schema:Date"
      ],
      "base": "XSDDate",
      "uri": "http://www.w3.org/2001/XMLSchema#date",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "datetime",
      "definition_uri": "https://w3id.org/linkml/Datetime",
      "description": "The combination of a date and time",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "exact_mappings": [
        "schema:DateTime"
      ],
      "base": "XSDDateTime",
      "uri": "http://www.w3.org/2001/XMLSchema#dateTime",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "date_or_datetime",
      "definition_uri": "https://w3id.org/linkml/DateOrDatetime",
      "description": "Either a date or a datetime",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "base": "str",
      "uri": "https://w3id.org/linkml/DateOrDatetime",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "uriorcurie",
      "definition_uri": "https://w3id.org/linkml/Uriorcurie",
      "description": "a URI or a CURIE",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "base": "URIorCURIE",
      "uri": "http://www.w3.org/2001/XMLSchema#anyURI",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "curie",
      "definition_uri": "https://w3id.org/linkml/Curie",
      "conforms_to": "https://www.w3.org/TR/curie/",
      "description": "a compact URI",
      "comments": [
        "in RDF serializations this MUST be expanded to a URI",
        "in non-RDF serializations MAY be serialized as the compact representation"
      ],
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "base": "Curie",
      "uri": "http://www.w3.org/2001/XMLSchema#string",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "uri",
      "definition_uri": "https://w3id.org/linkml/Uri",
      "conforms_to": "https://www.ietf.org/rfc/rfc3987.txt",
      "description": "a complete URI",
      "comments": [
        "in RDF serializations a slot with range of uri is treated as a literal or type xsd:anyURI unless it is an identifier or a reference to an identifier, in which case it is translated directly to a node"
      ],
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "close_mappings": [
        "schema:URL"
      ],
      "base": "URI",
      "uri": "http://www.w3.org/2001/XMLSchema#anyURI",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "ncname",
      "definition_uri": "https://w3id.org/linkml/Ncname",
      "description": "Prefix part of CURIE",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "base": "NCName",
      "uri": "http://www.w3.org/2001/XMLSchema#string",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "objectidentifier",
      "definition_uri": "https://w3id.org/linkml/Objectidentifier",
      "description": "A URI or CURIE that represents an object in the model.",
      "comments": [
        "Used for inheritence and type checking"
      ],
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "base": "ElementIdentifier",
      "uri": "http://www.w3.org/ns/shex#iri",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "nodeidentifier",
      "definition_uri": "https://w3id.org/linkml/Nodeidentifier",
      "description": "A URI, CURIE or BNODE that represents a node in a model.",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "base": "NodeIdentifier",
      "uri": "http://www.w3.org/ns/shex#nonLiteral",
      "repr": "str",
      "@type": "TypeDefinition"
    }
  ],
  "enums": [
    {
      "name": "ValidBenchmarkProblem",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/ValidBenchmarkProblem",
      "description": "Known parts of the accepted PFHub Benchmark Problems.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "permissible_values": [
        {
          "text": "1a",
          "description": "Spinodal decomposition in a square domain with periodic boundaries."
        },
        {
          "text": "1b",
          "description": "Spinodal decomposition in a square domain with no-flux boundaries."
        },
        {
          "text": "1c",
          "description": "Spinodal decomposition in a T-shaped domain with no-flux boundaries."
        },
        {
          "text": "1d",
          "description": "Spinodal decomposition on a closed spherical shell."
        },
        {
          "text": "2a",
          "description": "Ostwald ripening in a square domain with periodic boundaries."
        },
        {
          "text": "2b",
          "description": "Ostwald ripening in a square domain with no-flux boundaries."
        },
        {
          "text": "2c",
          "description": "Ostwald ripening in a T-shaped domain with no-flux boundaries."
        },
        {
          "text": "2d",
          "description": "Ostwald ripening on a closed spherical shell."
        },
        {
          "text": "3a",
          "description": "Dendritic growth in a square domain."
        },
        {
          "text": "4a",
          "description": "Elastic precipitate with radii (20 nm, 20 nm), C\u2081\u2081\u2081\u2081=250 aJ/nm\u00b3, C\u2081\u2081\u2082\u2082=150 aJ/nm\u00b3, C\u2081\u2082\u2081\u2082=100 aJ/nm\u00b3."
        },
        {
          "text": "4b",
          "description": "Elastic precipitate with radii (75 nm, 75 nm), C\u2081\u2081\u2081\u2081=250 aJ/nm\u00b3, C\u2081\u2081\u2082\u2082=150 aJ/nm\u00b3, C\u2081\u2082\u2081\u2082=100 aJ/nm\u00b3."
        },
        {
          "text": "4c",
          "description": "Elastic precipitate with radii (20 nm, 20 nm), C\u2081\u2081\u2081\u2081=275 aJ/nm\u00b3, C\u2081\u2081\u2082\u2082=165 aJ/nm\u00b3, C\u2081\u2082\u2081\u2082=110 aJ/nm\u00b3."
        },
        {
          "text": "4d",
          "description": "Elastic precipitate with radii (75 nm, 75 nm), C\u2081\u2081\u2081\u2081=275 aJ/nm\u00b3, C\u2081\u2081\u2082\u2082=165 aJ/nm\u00b3, C\u2081\u2082\u2081\u2082=110 aJ/nm\u00b3."
        },
        {
          "text": "4e",
          "description": "Elastic precipitate with radii (20/0.9 nm, 0.9*20 nm), C\u2081\u2081\u2081\u2081=250 aJ/nm\u00b3, C\u2081\u2081\u2082\u2082=150 aJ/nm\u00b3, C\u2081\u2082\u2081\u2082=100 aJ/nm\u00b3."
        },
        {
          "text": "4f",
          "description": "Elastic precipitate with radii (75/0.9 nm, 0.9*75 nm), C\u2081\u2081\u2081\u2081=250 aJ/nm\u00b3, C\u2081\u2081\u2082\u2082=150 aJ/nm\u00b3, C\u2081\u2082\u2081\u2082=100 aJ/nm\u00b3."
        },
        {
          "text": "4g",
          "description": "Elastic precipitate with radii (20/0.9 nm, 0.9*20 nm), C\u2081\u2081\u2081\u2081=275 aJ/nm\u00b3, C\u2081\u2081\u2082\u2082=165 aJ/nm\u00b3, C\u2081\u2082\u2081\u2082=110 aJ/nm\u00b3."
        },
        {
          "text": "4h",
          "description": "Elastic precipitate with radii (75/0.9 nm, 0.9*75 nm), C\u2081\u2081\u2081\u2081=275 aJ/nm\u00b3, C\u2081\u2081\u2082\u2082=165 aJ/nm\u00b3, C\u2081\u2082\u2081\u2082=110 aJ/nm\u00b3."
        },
        {
          "text": "5a",
          "description": "Stokes flow in an un-obstructed channel."
        },
        {
          "text": "5b",
          "description": "Stokes flow in a channel with elliptical obstruction."
        },
        {
          "text": "6a",
          "description": "Electrostatics in a square domain."
        },
        {
          "text": "6b",
          "description": "Electrostatics in a domain comprised of a rectangle and half-circle."
        },
        {
          "text": "7a",
          "description": "Method of Manufactured Solutions: order of accuracy test."
        },
        {
          "text": "7b",
          "description": "Method of Manufactured Solutions: performance with fixed error."
        },
        {
          "text": "7c",
          "description": "Method of Manufactured Solutions: increased rate of change."
        },
        {
          "text": "8a",
          "description": "Homogeneous nucleation with a single initial seed."
        },
        {
          "text": "8b",
          "description": "Homogeneous nucleation with multiple initial seeds."
        },
        {
          "text": "8c",
          "description": "Homogeneous nucleation with seeds at random times."
        }
      ]
    },
    {
      "name": "ValidBenchmarkVersion",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/ValidBenchmarkVersion",
      "description": "Known versions of the accepted PFHub Benchmark Problems.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "permissible_values": [
        {
          "text": "0",
          "description": "Initial or Hackathon version."
        },
        {
          "text": "1",
          "description": "Published version."
        }
      ]
    },
    {
      "name": "CsvFileTypes",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/CsvFileTypes",
      "description": "Valid CSV file extensions.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "permissible_values": [
        {
          "text": "csv",
          "description": "Comma-separated values.",
          "meaning": "https://www.iana.org/assignments/media-types/text/csv"
        }
      ]
    },
    {
      "name": "TarballTypes",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/TarballTypes",
      "description": "Valid tarball file extensions.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "permissible_values": [
        {
          "text": "tgz",
          "description": "Shorthand tarball extension.",
          "meaning": "https://www.iana.org/assignments/media-types/application/x-gtar"
        },
        {
          "text": "tar.gz",
          "description": "Gzipped Tar extension.",
          "meaning": "https://www.iana.org/assignments/media-types/application/x-gtar"
        }
      ]
    }
  ],
  "slots": [
    {
      "name": "id",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/id",
      "description": "Unique identifier that must not collide with any other.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "https://schema.org/identifier"
      ],
      "slot_uri": "https://schema.org/identifier",
      "identifier": true,
      "owner": "Contributor",
      "domain_of": [
        "BenchmarkResult",
        "Contributor"
      ],
      "range": "uriorcurie",
      "required": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "affiliation",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/affiliation",
      "description": "Institution, company, or agency a person works with.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "slot_uri": "https://w3id.org/usnistgov/pfhub-schema/affiliation",
      "multivalued": true,
      "owner": "Contributor",
      "domain_of": [
        "Contributor"
      ],
      "range": "string",
      "inlined_as_list": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "architecture",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/architecture",
      "description": "Computer processor architecture required for this simulation.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://www.fairsharing.org/ontology/subject/SRAO_0000258"
      ],
      "slot_uri": "http://www.fairsharing.org/ontology/subject/SRAO_0000258",
      "owner": "ComputeResource",
      "domain_of": [
        "ComputeResource"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "benchmark_problem",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/benchmark_problem",
      "description": "Alphanumerical identifier for part of a Benchmark Problem.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "slot_uri": "https://w3id.org/usnistgov/pfhub-schema/benchmark_problem",
      "owner": "BenchmarkResult",
      "domain_of": [
        "BenchmarkResult"
      ],
      "range": "ValidBenchmarkProblem",
      "required": true,
      "pattern": "^\\d\\a",
      "@type": "SlotDefinition"
    },
    {
      "name": "benchmark_version",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/benchmark_version",
      "description": "Numerical identifier for a Benchmark Problem version.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "slot_uri": "https://w3id.org/usnistgov/pfhub-schema/benchmark_version",
      "owner": "BenchmarkResult",
      "domain_of": [
        "BenchmarkResult"
      ],
      "range": "integer",
      "required": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "columns",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/columns",
      "description": "Column names applied to tabular data.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "slot_uri": "https://w3id.org/usnistgov/pfhub-schema/columns",
      "multivalued": true,
      "owner": "CsvFile",
      "domain_of": [
        "CsvFile"
      ],
      "range": "string",
      "inlined_as_list": true,
      "pattern": "^\\S+",
      "@type": "SlotDefinition"
    },
    {
      "name": "commit",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/commit",
      "description": "Commit hash in a repository of source code.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "https://en.wikipedia.org/wiki/Commit_(version_control)"
      ],
      "slot_uri": "https://en.wikipedia.org/wiki/Commit_(version_control)",
      "owner": "SourceCode",
      "domain_of": [
        "Software",
        "SourceCode"
      ],
      "range": "string",
      "pattern": "^\\S+",
      "@type": "SlotDefinition"
    },
    {
      "name": "cores",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/cores",
      "description": "Number of parallel cores or threads of execution used by the simulation on a CPU, GPU, or accelerator device.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://purl.obolibrary.org/obo/NCIT_C64194"
      ],
      "slot_uri": "http://purl.obolibrary.org/obo/NCIT_C64194",
      "owner": "ComputeResource",
      "domain_of": [
        "ComputeResource"
      ],
      "range": "integer",
      "@type": "SlotDefinition"
    },
    {
      "name": "date_created",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/date_created",
      "description": "Day when an item was uploaded, released, or published.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "https://schema.org/dateCreated"
      ],
      "slot_uri": "https://schema.org/dateCreated",
      "owner": "BenchmarkResult",
      "domain_of": [
        "BenchmarkResult"
      ],
      "range": "date",
      "@type": "SlotDefinition"
    },
    {
      "name": "download",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/download",
      "description": "Link to a download URL.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "https://schema.org/downloadUrl"
      ],
      "slot_uri": "https://schema.org/downloadUrl",
      "owner": "Software",
      "domain_of": [
        "Software"
      ],
      "range": "uriorcurie",
      "@type": "SlotDefinition"
    },
    {
      "name": "email",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/email",
      "description": "A person's electronic mailing address.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "https://schema.org/email"
      ],
      "slot_uri": "https://schema.org/email",
      "owner": "Contributor",
      "domain_of": [
        "Contributor"
      ],
      "range": "string",
      "pattern": "^\\S+@[\\S+\\.]+\\S+",
      "@type": "SlotDefinition"
    },
    {
      "name": "format",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/format",
      "description": "Format of data contained in a file.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "https://schema.org/encodingFormat"
      ],
      "slot_uri": "https://schema.org/encodingFormat",
      "owner": "File",
      "domain_of": [
        "File"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "handle",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/handle",
      "description": "A person's identifier on a website, without the \"at\" symbol.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "https://schema.org/member"
      ],
      "slot_uri": "https://schema.org/member",
      "multivalued": true,
      "owner": "Contributor",
      "domain_of": [
        "Contributor"
      ],
      "range": "uriorcurie",
      "inlined_as_list": true,
      "@type": "SlotDefinition"
