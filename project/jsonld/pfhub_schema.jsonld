{
  "name": "pfhub_schema",
  "description": "Schema for Phase-field Simulation and Benchmark Results schema in LinkML.",
  "title": "PFHub Benchmark Results",
  "see_also": [
    "https://pages.nist.gov/pfhub",
    "https://pages.nist.gov/pfhub-schema"
  ],
  "id": "https://w3id.org/usnistgov/pfhub-schema",
  "version": "0.1.0",
  "imports": [
    "linkml:types"
  ],
  "license": "https://www.nist.gov/open/license",
  "prefixes": [
    {
      "prefix_prefix": "pfhub",
      "prefix_reference": "https://w3id.org/usnistgov/pfhub-schema/"
    },
    {
      "prefix_prefix": "linkml",
      "prefix_reference": "https://w3id.org/linkml/"
    },
    {
      "prefix_prefix": "dcterms",
      "prefix_reference": "http://purl.org/dc/terms/"
    },
    {
      "prefix_prefix": "doi",
      "prefix_reference": "https://doi.org/"
    },
    {
      "prefix_prefix": "iana_text",
      "prefix_reference": "https://www.iana.org/assignments/media-types/text/"
    },
    {
      "prefix_prefix": "iana_app",
      "prefix_reference": "https://www.iana.org/assignments/media-types/application/"
    },
    {
      "prefix_prefix": "NCIT",
      "prefix_reference": "http://purl.obolibrary.org/obo/NCIT_"
    },
    {
      "prefix_prefix": "orcid",
      "prefix_reference": "https://orcid.org/"
    },
    {
      "prefix_prefix": "qudt",
      "prefix_reference": "http://qudt.org/schema/qudt/"
    },
    {
      "prefix_prefix": "schema",
      "prefix_reference": "http://schema.org/"
    },
    {
      "prefix_prefix": "SRAO",
      "prefix_reference": "http://www.fairsharing.org/ontology/subject/SRAO_"
    },
    {
      "prefix_prefix": "UO",
      "prefix_reference": "http://purl.obolibrary.org/obo/UO_"
    },
    {
      "prefix_prefix": "bitbucket",
      "prefix_reference": "https://bitbucket.org/"
    },
    {
      "prefix_prefix": "github",
      "prefix_reference": "https://github.com/"
    },
    {
      "prefix_prefix": "gitlab",
      "prefix_reference": "https://gitlab.com/"
    }
  ],
  "default_curi_maps": [
    "semweb_context"
  ],
  "default_prefix": "pfhub",
  "default_range": "string",
  "types": [
    {
      "name": "string",
      "definition_uri": "https://w3id.org/linkml/String",
      "description": "A character string",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "exact_mappings": [
        "schema:Text"
      ],
      "base": "str",
      "uri": "http://www.w3.org/2001/XMLSchema#string",
      "@type": "TypeDefinition"
    },
    {
      "name": "integer",
      "definition_uri": "https://w3id.org/linkml/Integer",
      "description": "An integer",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "exact_mappings": [
        "schema:Integer"
      ],
      "base": "int",
      "uri": "http://www.w3.org/2001/XMLSchema#integer",
      "@type": "TypeDefinition"
    },
    {
      "name": "boolean",
      "definition_uri": "https://w3id.org/linkml/Boolean",
      "description": "A binary (true or false) value",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "exact_mappings": [
        "schema:Boolean"
      ],
      "base": "Bool",
      "uri": "http://www.w3.org/2001/XMLSchema#boolean",
      "repr": "bool",
      "@type": "TypeDefinition"
    },
    {
      "name": "float",
      "definition_uri": "https://w3id.org/linkml/Float",
      "description": "A real number that conforms to the xsd:float specification",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "exact_mappings": [
        "schema:Float"
      ],
      "base": "float",
      "uri": "http://www.w3.org/2001/XMLSchema#float",
      "@type": "TypeDefinition"
    },
    {
      "name": "double",
      "definition_uri": "https://w3id.org/linkml/Double",
      "description": "A real number that conforms to the xsd:double specification",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "close_mappings": [
        "schema:Float"
      ],
      "base": "float",
      "uri": "http://www.w3.org/2001/XMLSchema#double",
      "@type": "TypeDefinition"
    },
    {
      "name": "decimal",
      "definition_uri": "https://w3id.org/linkml/Decimal",
      "description": "A real number with arbitrary precision that conforms to the xsd:decimal specification",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "broad_mappings": [
        "schema:Number"
      ],
      "base": "Decimal",
      "uri": "http://www.w3.org/2001/XMLSchema#decimal",
      "@type": "TypeDefinition"
    },
    {
      "name": "time",
      "definition_uri": "https://w3id.org/linkml/Time",
      "description": "A time object represents a (local) time of day, independent of any particular day",
      "notes": [
        "URI is dateTime because OWL reasoners do not work with straight date or time"
      ],
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "exact_mappings": [
        "schema:Time"
      ],
      "base": "XSDTime",
      "uri": "http://www.w3.org/2001/XMLSchema#dateTime",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "date",
      "definition_uri": "https://w3id.org/linkml/Date",
      "description": "a date (year, month and day) in an idealized calendar",
      "notes": [
        "URI is dateTime because OWL reasoners don't work with straight date or time"
      ],
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "exact_mappings": [
        "schema:Date"
      ],
      "base": "XSDDate",
      "uri": "http://www.w3.org/2001/XMLSchema#date",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "datetime",
      "definition_uri": "https://w3id.org/linkml/Datetime",
      "description": "The combination of a date and time",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "exact_mappings": [
        "schema:DateTime"
      ],
      "base": "XSDDateTime",
      "uri": "http://www.w3.org/2001/XMLSchema#dateTime",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "date_or_datetime",
      "definition_uri": "https://w3id.org/linkml/DateOrDatetime",
      "description": "Either a date or a datetime",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "base": "str",
      "uri": "https://w3id.org/linkml/DateOrDatetime",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "uriorcurie",
      "definition_uri": "https://w3id.org/linkml/Uriorcurie",
      "description": "a URI or a CURIE",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "base": "URIorCURIE",
      "uri": "http://www.w3.org/2001/XMLSchema#anyURI",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "curie",
      "definition_uri": "https://w3id.org/linkml/Curie",
      "conforms_to": "https://www.w3.org/TR/curie/",
      "description": "a compact URI",
      "comments": [
        "in RDF serializations this MUST be expanded to a URI",
        "in non-RDF serializations MAY be serialized as the compact representation"
      ],
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "base": "Curie",
      "uri": "http://www.w3.org/2001/XMLSchema#string",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "uri",
      "definition_uri": "https://w3id.org/linkml/Uri",
      "conforms_to": "https://www.ietf.org/rfc/rfc3987.txt",
      "description": "a complete URI",
      "comments": [
        "in RDF serializations a slot with range of uri is treated as a literal or type xsd:anyURI unless it is an identifier or a reference to an identifier, in which case it is translated directly to a node"
      ],
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "close_mappings": [
        "schema:URL"
      ],
      "base": "URI",
      "uri": "http://www.w3.org/2001/XMLSchema#anyURI",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "ncname",
      "definition_uri": "https://w3id.org/linkml/Ncname",
      "description": "Prefix part of CURIE",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "base": "NCName",
      "uri": "http://www.w3.org/2001/XMLSchema#string",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "objectidentifier",
      "definition_uri": "https://w3id.org/linkml/Objectidentifier",
      "description": "A URI or CURIE that represents an object in the model.",
      "comments": [
        "Used for inheritence and type checking"
      ],
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "base": "ElementIdentifier",
      "uri": "http://www.w3.org/ns/shex#iri",
      "repr": "str",
      "@type": "TypeDefinition"
    },
    {
      "name": "nodeidentifier",
      "definition_uri": "https://w3id.org/linkml/Nodeidentifier",
      "description": "A URI, CURIE or BNODE that represents a node in a model.",
      "from_schema": "https://w3id.org/linkml/types",
      "imported_from": "linkml:types",
      "base": "NodeIdentifier",
      "uri": "http://www.w3.org/ns/shex#nonLiteral",
      "repr": "str",
      "@type": "TypeDefinition"
    }
  ],
  "slots": [
    {
      "name": "id",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/id",
      "description": "Unique identifier that must not collide with any other.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://schema.org/identifier"
      ],
      "slot_uri": "http://schema.org/identifier",
      "identifier": true,
      "owner": "Contributor",
      "domain_of": [
        "BenchmarkResult",
        "BenchmarkProblem",
        "Contributor"
      ],
      "range": "uriorcurie",
      "required": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "affiliation",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/affiliation",
      "description": "Institution, company, or agency a person works with.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "slot_uri": "https://w3id.org/usnistgov/pfhub-schema/affiliation",
      "multivalued": true,
      "owner": "Contributor",
      "domain_of": [
        "Contributor"
      ],
      "range": "string",
      "inlined_as_list": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "architecture",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/architecture",
      "description": "Computer processor architecture required for this simulation.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://www.fairsharing.org/ontology/subject/SRAO_0000258"
      ],
      "slot_uri": "http://www.fairsharing.org/ontology/subject/SRAO_0000258",
      "owner": "ComputeResource",
      "domain_of": [
        "ComputeResource"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "columns",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/columns",
      "description": "Column names applied to tabular data.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "slot_uri": "https://w3id.org/usnistgov/pfhub-schema/columns",
      "multivalued": true,
      "owner": "CsvFile",
      "domain_of": [
        "CsvFile"
      ],
      "range": "string",
      "inlined_as_list": true,
      "pattern": "^\\S+",
      "@type": "SlotDefinition"
    },
    {
      "name": "cores",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/cores",
      "description": "Number of parallel cores or threads of execution used by the simulation on a CPU, GPU, or accelerator device.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://purl.obolibrary.org/obo/NCIT_C64194"
      ],
      "slot_uri": "http://purl.obolibrary.org/obo/NCIT_C64194",
      "owner": "ComputeResource",
      "domain_of": [
        "ComputeResource"
      ],
      "range": "integer",
      "@type": "SlotDefinition"
    },
    {
      "name": "date_created",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/date_created",
      "description": "Day when an item was uploaded, released, or published.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://schema.org/dateCreated"
      ],
      "slot_uri": "http://schema.org/dateCreated",
      "owner": "BenchmarkResult",
      "domain_of": [
        "BenchmarkResult"
      ],
      "range": "date",
      "@type": "SlotDefinition"
    },
    {
      "name": "download",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/download",
      "description": "Link to a download URL.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://schema.org/downloadUrl"
      ],
      "slot_uri": "http://schema.org/downloadUrl",
      "owner": "Software",
      "domain_of": [
        "Software"
      ],
      "range": "uriorcurie",
      "@type": "SlotDefinition"
    },
    {
      "name": "email",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/email",
      "description": "A person's electronic mailing address.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://schema.org/email"
      ],
      "slot_uri": "http://schema.org/email",
      "owner": "Contributor",
      "domain_of": [
        "Contributor"
      ],
      "range": "string",
      "pattern": "^\\S+@[\\S+\\.]+\\S+",
      "@type": "SlotDefinition"
    },
    {
      "name": "format",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/format",
      "description": "Format of data contained in a file.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://schema.org/encodingFormat"
      ],
      "slot_uri": "http://schema.org/encodingFormat",
      "owner": "File",
      "domain_of": [
        "File"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "handle",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/handle",
      "description": "A person's identifier on a website, without the \"at\" symbol.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://schema.org/member"
      ],
      "slot_uri": "http://schema.org/member",
      "multivalued": true,
      "owner": "Contributor",
      "domain_of": [
        "Contributor"
      ],
      "range": "uriorcurie",
      "inlined_as_list": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "memory_in_kb",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/memory_in_kb",
      "description": "Memory capacity required for this simulation.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "slot_uri": "https://w3id.org/usnistgov/pfhub-schema/memory_in_kb",
      "owner": "Results",
      "domain_of": [
        "Results"
      ],
      "range": "integer",
      "unit": {
        "ucum_code": "KIB*BY",
        "has_quantity_kind": "UO:0000245",
        "@type": "UnitOfMeasure"
      },
      "@type": "SlotDefinition"
    },
    {
      "name": "name",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/name",
      "description": "Informal or human-memorable identifier that may collide with others.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://schema.org/name"
      ],
      "slot_uri": "http://schema.org/name",
      "owner": "SourceCode",
      "domain_of": [
        "BenchmarkResult",
        "BenchmarkProblem",
        "Contributor",
        "File",
        "Software",
        "SourceCode"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "nodes",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/nodes",
      "description": "Number of machines allocated for the simulation in a computer cluster environment.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://purl.obolibrary.org/obo/NCIT_C18132"
      ],
      "slot_uri": "http://purl.obolibrary.org/obo/NCIT_C18132",
      "owner": "ComputeResource",
      "domain_of": [
        "ComputeResource"
      ],
      "range": "integer",
      "@type": "SlotDefinition"
    },
    {
      "name": "repository",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/repository",
      "description": "A repository of source code.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://schema.org/codeRepository"
      ],
      "slot_uri": "http://schema.org/codeRepository",
      "owner": "SourceCode",
      "domain_of": [
        "Software",
        "SourceCode"
      ],
      "range": "uriorcurie",
      "@type": "SlotDefinition"
    },
    {
      "name": "summary",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/summary",
      "description": "Text description, abstract, or narrative.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://schema.org/abstract"
      ],
      "slot_uri": "http://schema.org/abstract",
      "owner": "BenchmarkResult",
      "domain_of": [
        "BenchmarkResult"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "version",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/version",
      "description": "Semantic version of a software project.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://schema.org/softwareVersion"
      ],
      "slot_uri": "http://schema.org/softwareVersion",
      "owner": "Software",
      "domain_of": [
        "BenchmarkProblem",
        "Software"
      ],
      "range": "string",
      "recommended": true,
      "pattern": "^[\\d+\\.]+",
      "@type": "SlotDefinition"
    },
    {
      "name": "time_in_s",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/time_in_s",
      "description": "Amount of time required to execute the simulation.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://schema.org/Number"
      ],
      "slot_uri": "http://schema.org/Number",
      "owner": "Results",
      "domain_of": [
        "Results"
      ],
      "range": "integer",
      "unit": {
        "ucum_code": "S",
        "has_quantity_kind": "NCIT:C42535",
        "@type": "UnitOfMeasure"
      },
      "@type": "SlotDefinition"
    },
    {
      "name": "url",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/url",
      "description": "URL of an online resource or website.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://schema.org/url"
      ],
      "slot_uri": "http://schema.org/url",
      "owner": "SourceCode",
      "domain_of": [
        "Software",
        "SourceCode"
      ],
      "range": "uriorcurie",
      "@type": "SlotDefinition"
    },
    {
      "name": "benchmark",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/benchmark",
      "description": "Details of the Benchmark Problem.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "slot_uri": "https://w3id.org/usnistgov/pfhub-schema/benchmark",
      "owner": "BenchmarkResult",
      "domain_of": [
        "BenchmarkResult"
      ],
      "range": "BenchmarkProblem",
      "inlined": true,
      "inlined_as_list": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "contributors",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/contributors",
      "description": "Collection of people who worked on this solution.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://schema.org/contributor"
      ],
      "slot_uri": "http://schema.org/contributor",
      "multivalued": true,
      "owner": "BenchmarkResult",
      "domain_of": [
        "BenchmarkResult"
      ],
      "range": "Contributor",
      "inlined": true,
      "inlined_as_list": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "framework",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/framework",
      "description": "A computer program or library",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "mappings": [
        "http://schema.org/SoftwareApplication"
      ],
      "slot_uri": "http://schema.org/SoftwareApplication",
      "multivalued": true,
      "owner": "BenchmarkResult",
      "domain_of": [
        "BenchmarkResult"
      ],
      "range": "Software",
      "inlined": true,
      "inlined_as_list": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "hardware",
      "definition_uri": "https://w3id.org/usnistgov/pfhub-schema/hardware",
      "description": "Computer hardware used to run the simulation.",
      "from_schema": "https://w3id.org/usnistgov/pfhub-schema",
      "slot_uri": "https://w3id.org/usnistgov/pfhub-schema/hardware",
      "multivalued": true,
      "owner": "Results",
      "domain_of": [
        "Results"
      ],
      "range": "ComputeResource",
      "inlined": true,
      "@type": "SlotDefinition"
